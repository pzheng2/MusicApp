<h1>Log In</h1>
<br>

<form method="POST" action="<%= session_url %>">
  <input type="hidden"
         name="authenticity_token"
         value="<%= form_authenticity_token %>">
  <label>Email
    <input type="text" name="user[email]">
  </label>

  <label>Password
    <input type="password" name="user[password]">
  </label>
  <button type="submit">Log In</button>
</form>
